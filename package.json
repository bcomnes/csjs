{
  "name": "csjs",
  "version": "1.0.6",
  "description": "Cascading Style JavaScripts",
  "keywords": [
    "csjs",
    "css-modules",
    "scoped-css",
    "css-in-js",
    "modular-css",
    "css"
  ],
  "author": "Ryan Tsao <ryan.j.tsao@gmail.com>",
  "main": "dist/index.js",
  "module": "dist-es/index.js",
  "homepage": "https://github.com/rtsao/csjs",
  "repository": "git@github.com:rtsao/csjs.git",
  "bugs": "https://github.com/rtsao/csjs/issues",
  "scripts": {
    "prepare": "npm run transpile",
    "transpile": "babel src -d dist --copy-files",
    "rollup": "rollup src/index.js -o dist-es/index.js -f esm",
    "transpile-rollup": "BABEL_ENV=rollup babel dist-es/index.js -o dist-es/index.es5.js -s",
    "build": "npm run rollup && npm run transpile-rollup",
    "test": "npm run transpile && node dist/test/index.js",
    "cover": "nyc --reporter=lcov node dist/test/index.js",
    "travis-test": "npm run cover && ((cat coverage/lcov.info | coveralls) || exit 0)",
    "travis-test-browser": "browserify dist/test/index.js -t bulkify -t brfs -t folderify | tape-run"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "brfs": "^2.0.0",
    "browserify": "^16.5.0",
    "bulk-require": "^1.0.1",
    "bulkify": "^1.1.1",
    "coveralls": "^3.0.6",
    "folderify": "^1.2.1",
    "include-folder": "^1.0.0",
    "nyc": "^14.1.1",
    "rollup": "^1.19.4",
    "tape": "^4.2.0",
    "tape-run": "^6.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT"
}
